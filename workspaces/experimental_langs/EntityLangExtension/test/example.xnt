module example

imports A
 
entity User {
  name     : String
  password : String
  homepage : URL
}

entity BlogPosting {
  poster : User
  body   : String 
} 

entity URL {
  location : Xml 
}

def post(): Xml{
	var blogPost: Xml;
	var content: String;
	content     := "Sample Blogpost!";
	
	blogPost := <html>
					<h1>BlogPost</h1>
					<body>Welcome. <[content]> </body>
				</html>;
	return blogPost;
}